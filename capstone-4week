    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Midtown South']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Yorkville']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Lenox Hill']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Carnegie Hill']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Lincoln Square']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Marble Hill']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Murray Hill']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Hudson Yards']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Roosevelt Island']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Stuyvesant Town']\n",
    "manhattan_data = manhattan_data[manhattan_data.iloc[:, 0] != 'Manhattan Valley'].reset_index(drop=True)"
   ]
