    "CLIENT_ID = 'HVTQKZEJT5FFAIZILSXYOGCT0ICKPG00CWIFLNRKJR0US55P' # your Foursquare ID\n",
    "CLIENT_SECRET = 'CFOAEGK4YNF3UL5K1QGAOET4UZ1EXA1FHVCLSWA4WTBOLYMM' # your Foursquare Secret\n",
    "VERSION = '20180605' # Foursquare API version"
