    "list_manhattan_neigh_beta[list_manhattan_neigh_beta.index(\"Upper West Side\")] = \"Upper West Side (UWS)\"\n",
    "list_manhattan_neigh_beta[list_manhattan_neigh_beta.index(\"Hell's Kitchen\")] = \"Hell's Kitchen (Clinton)\"\n",
    "list_manhattan_neigh_beta[list_manhattan_neigh_beta.index('Lower East Side')] = 'Lower East Side (LES)'\n",
