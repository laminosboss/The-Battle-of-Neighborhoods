   "source": [
    "# Create a ndarray object of the area data given as a list object\n",
    "area_array = np.asarray(area_list)\n",
    "# Compute the approximate radius of the neighborhoods using the area by means of the circle area formula\n",
    "radius_array = np.sqrt(area_array*1e6/(np.pi)).astype(np.int32)\n",
    "# Compute the population density of the neighborhhods using the population and the area data\n",
    "density_pop_array = np.divide(np.asarray(population_list), area_array).astype(np.int32)\n",
    "\n",
    "manhattan_data['Approx_Radius_Meters'] = pd.Series(radius_array, index=manhattan_data.index)\n",
    "manhattan_data['Population/Km2'] = pd.Series(density_pop_array, index=manhattan_data.index)\n",
    "manhattan_data['Median_Income_Dollars'] = pd.Series(np.asarray(income_list), index=manhattan_data.index)\n",
    "manhattan_data['Median_Rent_Dollars'] = pd.Series(np.asarray(rent_list), index=manhattan_data.index)\n",
    "\n",
    "manhattan_data"
   ]
